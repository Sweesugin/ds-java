
class Main{
    public static void main(String[] args) {
        child c=new child();
        c.hi();
        c.assest();
}}
abstract class parent {
    abstract void hi();
    void assest(){
        System.out.println("hi");
    }
}
class child extends parent{
      void hi(){
        System.out.println("Total Value ");
    }
}
  
