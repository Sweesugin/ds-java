import java.util.ArrayList;

class Complete {
    public static ArrayList<Integer> array(int[][] a, int[] b, int n) {
        ArrayList<Integer> result = new ArrayList<>();

        int diagonalSum = 0;
        int maxB = b[0];

        for (int i = 0; i < n; i++) {
            diagonalSum += a[i][i];
            if (b[i] > maxB) {
                maxB = b[i];
            }
        }

        result.add(diagonalSum);
        result.add(maxB);
        return result;
    }
}
