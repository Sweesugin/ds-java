public class UniqueDigits {
    public static void main(String[] args) {
        int number = 123321;

        boolean[] seen = new boolean[10];

        System.out.print("Unique digits: ");

        while (number > 0) {
            int digit = number % 10;   

            if (!seen[digit]) {       
                System.out.print(digit + " ");
                seen[digit] = true; 
            }
            number = number / 10;     
        }
    }
}
