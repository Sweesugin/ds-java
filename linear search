import java.util.Arrays;
class Main{
    public static void main(String [] args){
        int [] arr={3,4,5,5,6};
        boolean b=false;
        int k=7;
        for(int i=0;i<arr.length;i++){
            if(arr[i]==k){
                b=true;
                break;
            }
        }
        if(!b){
            System.out.println("Element !found");
        }else{
            System.out.print("Element Found");
        }
    }
}
