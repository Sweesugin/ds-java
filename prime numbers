public class UniqueDigits {
    public static void main(String[] args) {
        int number = 10;
        boolean isprime=true;
        for(int i=2;i<number;i++){
            if(number % i==0){
                isprime=false;
                break;}
}
if(isprime){
    System.out.print("Prime Number");
}else{
    System.out.println("Not a prime");
}
}}
