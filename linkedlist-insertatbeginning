class linked{
        node head;
        public void insert(int data){
            node temp=head;
        node newnode=new node( data);
        newnode.prev=head;
        head=newnode;
    }
    public void display(){
        node temp=head;
        while(temp!=null ){
            System.out.print(temp.data+">-");
             temp=temp.prev;
        }
       
    }
