class Solution {
    public int countDigits(int n) {
        // code here
        int c=0;
        while(n>0){
           n=n/10;
           c++;
        }return c;
    }
        public static void main(String [] args){
            Solution s=new Solution();
          System.out.println(s.countDigits(909));
            
        }
    }
