class Solution {
    static void calculate(int a, int b, int operator) {
        switch (operator) {
            case 1:
                System.out.println(a + b);
                break;
            case 2:
                System.out.println(a - b);
                break;
            case 3:
                System.out.println(a * b);
                break;
            default:
                System.out.print("Invalid Input");
        }
    }
}
